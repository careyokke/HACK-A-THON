<h2>Add a new member:</h2>

<form action="/members" method="post">
  <input id="teamId" name="teamId" type="hidden" value="$team.getId()">
  <label for="memberName">Please type your name </label>
  <input id="memberName" name="memberName" type="text">

  <button type="submit" class="btn btn-danger">Add new name</button>
</form>
